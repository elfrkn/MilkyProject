@model List<MilkyProject.WebUI.DTOs.Category.ResultCategoryDto>
<div class="container-xxl py-5">
    <div class="container">
        <div class="text-center mx-auto wow fadeInUp" data-wow-delay="0.1s" style="max-width: 500px;">
            <p class="section-title bg-white text-center text-primary px-3">Kategoriler</p>
            <h1 class="mb-5">
                Kategorilerimiz
            </h1>
        </div>
        <div class="row g-4">
            @foreach (var item in Model)
            {
                <div class="col-lg-6 col-md-6 wow fadeInUp" data-wow-delay="0.1s">
                    <div class="team-item rounded p-4">
                        <img class="img-fluid rounded mb-4" src="@item.ImageUrl" style="height:400px; width:800px;"alt="">
                        <h5>@item.categoryName</h5>
                    </div>
                </div>
            }

        </div>
    </div>
</div>
