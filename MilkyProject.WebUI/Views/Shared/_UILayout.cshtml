
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

@await Component.InvokeAsync("_UILayoutHeadComponentPartial")
<body>
    @await Component.InvokeAsync("_UISpinnerComponentPartial")
    @await Component.InvokeAsync("_UITopBarComponentPartial")
    @await Component.InvokeAsync("_UINavbarComponentPartial")
    @RenderBody()
    @await Component.InvokeAsync("_UIFooterComponentPartial")
    @await Component.InvokeAsync("_UICoprightComponentPartial")
    <a href="#" class="btn btn-lg btn-primary btn-lg-square rounded-circle back-to-top"><i class="bi bi-arrow-up"></i></a>
    @await Component.InvokeAsync("_UIScriptComponentPartial")
</body>

</html>